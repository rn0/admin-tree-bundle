<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="fsi_tree_node_move_up" pattern="/admin/{element}/{id}/reorder/move-up">
        <default key="_controller">fsi_tree.controller.reorder:moveUpAction</default>
    </route>

    <route id="fsi_tree_node_move_down" pattern="/admin/{element}/{id}/reorder/move-down">
        <default key="_controller">fsi_tree.controller.reorder:moveDownAction</default>
    </route>
</routes>
